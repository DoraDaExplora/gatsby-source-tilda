{
  "name": "gatsby-source-tilda",
  "version": "1.0.2",
  "description": "Gatsby source plugin for building websites using the tilda.cc CMS as a data source",
  "main": "index.js",
  "scripts": {
    "build": "babel src --out-dir . --ignore **/__tests__",
    "prepare": "cross-env NODE_ENV=production npm run build",
    "watch": "babel -w src --out-dir . --ignore **/__tests__"
  },
  "engines": {
    "node": ">=10.13.0"
  },
  "author": "dmalexeev",
  "license": "MIT",
  "keywords": [
    "gatsby",
    "gatsby-source",
    "tilda"
  ],
  "dependencies": {
    "@babel/runtime": "^7.8.7",
    "axios": "^0.19.2",
    "gatsby-source-filesystem": "^2.1.48",
    "is-online": "^8.2.1",
    "progress": "^2.0.3",
    "query-string": "^6.11.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.7",
    "babel-preset-gatsby-package": "^0.3.1",
    "cross-env": "^5.2.1"
  },
  "peerDependencies": {
    "gatsby": "^2.12.1"
  },
  "bugs": {
    "url": "https://github.com/dmalexeev/gatsby-source-tilda/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dmalexeev/gatsby-source-tilda.git"
  }
}
